{
  "name": "NewTab",
  "description": "Override the new tab page",
  "version": "0.1",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://raw.githubusercontent.com http://localhost:9999; object-src 'self'",
  "permissions": [
    "storage"
  ]
}
