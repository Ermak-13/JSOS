{
  "name": "JSOS",
  "description": "Override the new tab page",
  "version": "1.0",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://rawgit.com https://cdn.rawgit.com; object-src 'self'",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "storage",
    "tabs",
    "alarms"
  ]
}
