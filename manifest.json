{
  "name": "JSOS",
  "description": "Override the new tab page",
  "version": "0.1",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://rawgit.com https://cdn.rawgit.com; object-src 'self'",
  "permissions": [
    "storage"
  ]
}
